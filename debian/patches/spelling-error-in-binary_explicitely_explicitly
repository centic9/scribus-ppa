+	setMedia->setToolTip( "<qt>" + tr( "This enables you to explicitly set the media size of the PostScript file. Not recommended unless requested by your printer." ) + "</qt>");
 	connect(useAltPrintCommand, SIGNAL(clicked()), this, SLOT(selOtherComm()));
